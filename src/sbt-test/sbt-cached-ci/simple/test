> cachedCiTest
# B test depends on A test so it has to be newer than A
$ exists a/target/A.result
$ exists b/target/B.result
$ newer b/target/B.result a/target/A.result
$ delete a/target/A.result
> cachedCiTest
# nothing was executed
$ absent a/target/A.result
$ delete a/target/.lastCachedCiTestFull
> cachedCiTest
# A was executed but B remained old
$ exists a/target/A.result
$ exists b/target/B.result
$ newer a/target/A.result b/target/B.result
